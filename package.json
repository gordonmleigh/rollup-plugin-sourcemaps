{
  "name": "@gordonmleigh/rollup-plugin-sourcemaps",
  "version": "0.1.0",
  "type": "module",
  "author": {
    "name": "Gordon Leigh"
  },
  "description": "A Rollup plugin to load sourcemaps for input files",
  "keywords": [
    "rollup-plugin",
    "rollup",
    "sourcemaps"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gordonmleigh/rollup-plugin-sourcemaps"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && npm run compile && npm run lint",
    "clean": "rm -rf lib/ *.tsbuildinfo",
    "compile": "tsc",
    "lint": "eslint src/ --ext=ts"
  },
  "exports": {
    ".": {
      "types": "./plugin.d.ts",
      "default": "./plugin.js"
    }
  },
  "files": [
    "lib/"
  ],
  "dependencies": {
    "convert-source-map": "^2.0.0"
  },
  "peerDependencies": {
    "rollup": "^4"
  },
  "devDependencies": {
    "@types/convert-source-map": "^2.0.3",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.1",
    "rollup": "^4.9.1",
    "typecript": "^0.0.1-security"
  }
}
